[bumpr]
file = flask_restx/__about__.py
vcs = git
commit = true
tag = true
push = true
tests = tox
clean =
    inv clean
files =
    README.rst
    package.json

[bump]
unsuffix = true

[prepare]
part = patch
suffix = dev

[changelog]
file = CHANGELOG.rst
bump = {version} ({date:%Y-%m-%d})
prepare = Current

[readthedoc]
id = flask-restx

[replace]
dev = ?branch=master
stable = ?tag={version}
